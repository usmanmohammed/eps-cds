@model EPS.Models.CorpMember

@{
    ViewBag.Title = "CorpMemberDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="col-sm-9 col-md-10 admin-main">
    <div class="col-md-6 padd-top">
        <div class="well well-lg well-border">
            <h4>Corp Member Details</h4>
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Batch.BatchName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Batch.BatchName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.CorpMemberRole.Title)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.CorpMemberRole.Title)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Course.CourseName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Course.CourseName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.StateCode)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.StateCode)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.FullName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.FullName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.IsExco)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.IsExco)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Thumbnail)
                </dt>

                <dd>
                    <img class="test-image img-circle" src="data:@Model.ThumbnailContentType;base64,@System.Convert.ToBase64String(Model.Thumbnail)" />
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.DateOfBirth)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.DateOfBirth)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.CreatedBy)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.CreatedBy)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.DateCreated)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.DateCreated)
                </dd>
            </dl>
        </div>
    </div>
